{
    "name":        "dv-affection/deployment-builds-executor-bundle",
    "description": "Symfony bundle for deployment-builds-executor library",
    "type":        "symfony-bundle",
    "keywords":    [
        "symfony", "php", "deployment", "builds"
    ],
    "homepage":    "https://github.com/dVaffection/deployment-builds-executor-bundle",
    "license":     "MIT",
    "authors":     [
        {
            "name":  "dV",
            "email": "dVaffection@gmail.com"
        }
    ],
    "require":     {
        "php":                                     ">=5.3.3",
        "dv-affection/deployment-builds-executor": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit":              ">=3.7",
        "symfony/dependency-injection": "~2.2",
        "symfony/console":              "~2.2",
        "symfony/http-kernel":          "~2.2",
        "symfony/framework-bundle":     "~2.2"
    },
    "support":     {
        "issues": "https://github.com/dvAffection/deployment-builds-executor/issues",
        "source": "https://github.com/dvAffection/deployment-builds-executor"
    },
    "config":      {
        "bin-dir": "bin/"
    },
    "autoload":    {
        "psr-4": {
            "DvLab\\DeploymentBuildsExecutorBundle\\": "src/"
        }
    }
}
